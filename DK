-------- AUTOFARM

local farm = win:Server("Autofarm", "http://www.roblox.com/asset/?id=7874044")

local tgls = farm:Channel("Hatch x1")
tgls:Toggle("Forest Egg $75",false, function(bool)
    if bool == true then
        loadstring(game:HttpGet('https://pastebin.com/raw/uYvcVc5B'))()
    else
        loadstring(game:HttpGet('https://pastebin.com/raw/nFHx6Vpq'))()
    end
end)
    tgls:Toggle("Desert Egg $875",false, function(bool)
        if bool == true then
            loadstring(game:HttpGet('https://pastebin.com/raw/nQFsY2BS'))()
        else
            loadstring(game:HttpGet('https://pastebin.com/raw/nFHx6Vpq'))()
        end
    end)
        tgls:Toggle("Ocean Egg $8.000",false, function(bool)
            if bool == true then
                loadstring(game:HttpGet('https://pastebin.com/raw/QVzakaKq'))()
            else
                loadstring(game:HttpGet('https://pastebin.com/raw/nFHx6Vpq'))()
            end
        end)
            tgls:Toggle("Frozen Egg $75.000",false, function(bool)
                if bool == true then
                    loadstring(game:HttpGet('https://pastebin.com/raw/5PUS9rNQ'))()
                else
                    loadstring(game:HttpGet('https://pastebin.com/raw/nFHx6Vpq'))()
                end
            end)
                tgls:Toggle("Underworld Egg $3.000.000",false, function(bool)
                    if bool == true then
                        loadstring(game:HttpGet('https://pastebin.com/raw/1mkQF4ZL'))()
                    else
                        loadstring(game:HttpGet('https://pastebin.com/raw/nFHx6Vpq'))()
                    end
                end)
                    
